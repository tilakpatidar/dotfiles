[user]
	name = tilakpatidar
	email = tilakpatidar@gmail.com
  signingkey = 3F1C7463
[core]
	excludesfile = /Users/tilak/.gitignore_global
  commentChar = ";"
	autocrlf = input
# Enforce SSH
[url "ssh://git@github.com/"]
  insteadOf = https://github.com/
[commit]
  gpgsign = true
	template = /Users/tilak/.stCommitMsg
[tag]
  gpgsign = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[status]
  short = true
  branch = true
[gpg]
	program = gpg2
[init]
	templatedir = /Users/tilak/.git-template
